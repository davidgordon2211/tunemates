function insertCard(html) {
  cardResults.insertAdjacentHTML('beforeend', html);
}

var cardResults = document.getElementById('results');
cardResults.innerHTML = '';
<% @results.each do |song| %>
  <% if song.preview_url != nil %>
    insertCard('<%= j render "games/card", song: song, game: @game %>');
  <% end %>
<% end %>
